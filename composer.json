{
    "name": "laravel-admin-ext/mysql-editor",
    "description": "MySql editor extension for laravel-admin based on code-mirror",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "code mirror", "mysql", "editor"],
    "homepage": "https://github.com/laravel-admin-ext/mysql-editor",
    "license": "MIT",
    "authors": [
        {
            "name": "xudaoyang",
            "email": "xudaoyangwork@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "jxlwqq/code-mirror": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\PHPEditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\MysqlEditor\\MysqlEditorServiceProvider"
            ]
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
